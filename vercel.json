{
    "version": 2,
    "builds": [
        {
            "src": "src/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.py"
        }
    ],
    "devCommand": "uvicorn src.index:app --host 0.0.0.0 --port 3000"
}